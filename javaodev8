package giris;
import java.util.Scanner;
public class ciftsayilarıbul {
    public static void main(String[] args) {
        int k;
        Scanner sayi = new Scanner(System.in);

        System.out.println("Sayı giriniz : ");
        k=sayi.nextInt();
        int i =1;
        for (i=0;i<=k;i++)
        {
            if (i%3==0 && i%4==0){
                System.out.println(i);
            }
        }
    }
}
